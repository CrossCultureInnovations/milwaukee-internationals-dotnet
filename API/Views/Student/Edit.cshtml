@model Models.Entities.Student

@{
    Layout = "_Layout";
}

<div class="container">
    
    @using (Html.BeginForm("EditHandler", "Student", FormMethod.Post, new { ng_controller = "studentRegistrationCtrl", novalidate = "true" }))
    {
        @(Html.PartialAsync("StudentContent", Model, new ViewDataDictionary(ViewData) {{ "Type", "Edit"}}))
        
        <input class="btn btn-primary" type="submit" value="Update" />
    }
</div>